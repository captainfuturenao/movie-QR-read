{
    "version": 2,
    "outputDirectory": "out",
    "headers": [
        {
            "source": "/(.*).mp4",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "video/mp4"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}